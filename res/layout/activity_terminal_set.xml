<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include 
        android:id="@+id/top"
        layout="@layout/layout_title" />

     <ScrollView
        android:layout_below="@+id/top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none" >
        
         <LinearLayout
             android:layout_width="fill_parent"
             android:layout_height="wrap_content"
             android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/family_set_rl"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
        <ImageView
                    android:id="@+id/iv_icon_family"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_family" />
        

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@id/iv_icon_family"
            
            android:text="@string/family_num_set"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/share_table_indicator" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />

    <RelativeLayout
        android:id="@+id/stealth_set_rl"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
         <ImageView
                    android:id="@+id/iv_icon_hidden"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_hiden" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
              android:layout_marginLeft="5dp"
            android:layout_toRightOf="@id/iv_icon_hidden"
            android:layout_centerVertical="true"
            android:text="@string/stealth_set"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/share_table_indicator" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />

    <RelativeLayout
        android:id="@+id/whitelist_set_rl"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
         <ImageView
                    android:id="@+id/iv_icon_whitelist"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_whitelist" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/iv_icon_whitelist"
            android:layout_marginLeft="5dp"
            
            android:layout_centerVertical="true"
            android:text="@string/whitelist_set"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/share_table_indicator" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />

    <RelativeLayout
        android:id="@+id/location_mode_rl"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
          <ImageView
                    android:id="@+id/iv_icon_location"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_location" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:layout_toRightOf="@id/iv_icon_location"
            android:layout_marginLeft="5dp"
            android:layout_centerVertical="true"
            android:text="@string/location_mode"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/share_table_indicator" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />
    
     <RelativeLayout
        android:id="@+id/expel_mosq_ll"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
         <ImageView
                    android:id="@+id/iv_icon_drivemidge"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_drivemidge" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:layout_toRightOf="@id/iv_icon_drivemidge"
            android:layout_marginLeft="5dp"
            android:layout_centerVertical="true"
            android:text="@string/dis_expel_mosq_text"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/share_table_indicator" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />
    
     <RelativeLayout
        android:id="@+id/shutdown_ll"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
        <ImageView
                    android:id="@+id/iv_icon_shutdown"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_shutdown" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/iv_icon_shutdown"
            android:layout_marginLeft="5dp"
            android:layout_centerVertical="true"
            android:text="@string/dis_shutdown_text"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/share_table_indicator" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />
    
     <RelativeLayout
        android:id="@+id/findWatch_ll"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
        <ImageView
                    android:id="@+id/iv_icon_findwaltch"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_findwaltch" />
         

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:layout_toRightOf="@id/iv_icon_findwaltch"
            android:layout_marginLeft="5dp"
            android:layout_centerVertical="true"
            android:text="@string/dis_find_watch_text"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/share_table_indicator" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />
    
    

    <RelativeLayout
        android:id="@+id/excise_watch_warn_rl"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_common_click"
        android:paddingBottom="15.0dip"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:paddingTop="15.0dip" >
        <ImageView
                    android:id="@+id/iv_icon_picktable"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_picktable" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
             android:layout_toRightOf="@id/iv_icon_picktable"
            android:layout_marginLeft="5dp"
            android:text="@string/excise_watch_warn"
            android:textColor="@color/assict_text_color"
            android:textSize="16.0sp" />

        <CheckBox
            android:id="@+id/excise_watch_warn_cbox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/my_checkbox"
            android:button="@null"
            android:checked="false"
            android:clickable="false"
            android:enabled="false"
            android:focusable="false"
            android:focusableInTouchMode="false" />
    </RelativeLayout>


    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2"
        android:visibility="gone" />

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/home_icon_line2" />
    </LinearLayout>

     </ScrollView>
     
     <include layout="@layout/find_watch_popup" />
<include layout="@layout/dialog_shutdown" />

</RelativeLayout>